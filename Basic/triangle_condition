# check if 3 numbers forms a triangle or not

def triangle(side1,side2,side3):
  if(side1+side2 >=side3) and (side1+side3 >=side2) and (side3+side2 >=side1):
    result = "This is a triangle"
  else:
    result = "This is not a triangle"
  return result

if __name__ == "__main__":
  side1 = input("Enter side 1: ")
  side2 = input("Enter side 2: ")
  side3 = input("Enter side 3: ")
  result = triangle(side1,side2,side3)
  print(result)
